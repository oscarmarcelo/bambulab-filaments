{
	"name": "bambulab-filaments",
	"version": "0.1.0",
	"description": "A list of all of their current filaments sorted by their code.",
	"private": true,
	"type": "module",
	"main": "workflow/index.js",
	"scripts": {
		"start": "node .",
		"test": "xo",
		"clear": "node . clear",
		"publish": "node . publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oscarmarcelo/bambulab-filaments.git"
	},
	"author": "Oscar Marcelo (https://oscarmarcelo.com)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/oscarmarcelo/bambulab-filaments/issues"
	},
	"homepage": "https://github.com/oscarmarcelo/bambulab-filaments#readme",
	"devDependencies": {
		"acorn": "^8.14.0",
		"acorn-walk": "^8.3.4",
		"gh-pages": "^6.3.0",
		"puppeteer": "^24.2.0",
		"sharp": "^0.33.5",
		"xo": "^0.60.0"
	},
	"browserslist": [
		"defaults and last 2 years"
	],
	"xo": {
		"envs": [
			"node",
			"browser"
		],
		"rules": {
			"no-multiple-empty-lines": [
				"warn",
				{
					"max": 3
				}
			],
			"import/order": [
				"warn",
				{
					"newlines-between": "always"
				}
			],
			"unicorn/import-style": [
				"error",
				{
					"styles": {
						"node:path": {
							"named": true
						}
					}
				}
			]
		}
	}
}
